{
  "name": "zeppliear",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "check-types": "tsc --noEmit",
    "format": "prettier --write './**/*.{js,jsx,json,ts,tsx,html,css,md}'",
    "check-format": "prettier --check './**/*.{js,jsx,json,ts,tsx,html,css,md}'",
    "lint": "eslint --ext .ts,.tsx,.js,.jsx src/",
    "preview": "vite preview",
    "publish-worker-prod": "npm run wrangler-prod -- publish",
    "publish-worker-sandbox": "npm run wrangler-sandbox -- publish",
    "worker-logs-sandbox": "npm run wrangler-sandbox -- tail",
    "worker-logs-prod": "npm run wrangler-prod -- tail",
    "start-zero-cache": "cd ../../packages/zero-cache && npm run start -- dotenv_config_path=../../apps/zeppliear/.env",
    "start-zero-cache-single-thread": "SINGLE_PROCESS=1 npm run start-zero-cache"
  },
  "dependencies": {
    "@emotion/react": "^11.8.2",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^5.5.1",
    "@mui/material": "^5.5.2",
    "@mui/utils": "^5.4.4",
    "@tailwindcss/forms": "^0.5.0",
    "@tailwindcss/line-clamp": "^0.3.1",
    "@uidotdev/usehooks": "^2.4.1",
    "classnames": "^2.5.1",
    "fractional-indexing": "^3.0.1",
    "gzip-loader": "^0.0.1",
    "lodash": "^4.17.21",
    "nanoid": "^4.0.2",
    "react": ">=16.0 <19.0",
    "react-beautiful-dnd": "^13.1.0",
    "react-dom": ">=16.0 <19.0",
    "react-popper": "^2.2.5",
    "react-remark": "^2.1.0",
    "react-virtualized-auto-sizer": "^1.0.6",
    "react-window": "^1.8.6",
    "replicache-react": "^4.0.1",
    "zero-client": "0.0.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@rocicorp/eslint-config": "^0.5.1",
    "@rocicorp/prettier-config": "^0.2.0",
    "@svgr/webpack": "^6.2.1",
    "@tailwindcss/forms": "^0.5.0",
    "@tailwindcss/line-clamp": "^0.3.1",
    "@types/chai": "^4.3.5",
    "@types/lodash": "^4.14.197",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.16.0",
    "@types/react": "18.2.13",
    "@types/react-beautiful-dnd": "^13.1.2",
    "@types/react-dom": "18.2.6",
    "@types/react-virtualized-auto-sizer": "^1.0.1",
    "@types/react-window": "^1.8.5",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@vitejs/plugin-react": "^4.2.1",
    "chai": "^5.0.0-alpha.2",
    "concurrently": "^8.2.1",
    "dotenv": "^16.4.5",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "mocha": "^9.2.1",
    "postcss-preset-env": "^7.4.3",
    "prettier": "^3.0.3",
    "tailwindcss": "^3.0.23",
    "ts-node": "^10.9.1",
    "typescript": "^5.5.3",
    "vite": "^5.2.0",
    "vite-plugin-svgr": "^4.1.0"
  },
  "eslintConfig": {
    "extends": [
      "@rocicorp/eslint-config"
    ]
  },
  "prettier": "@rocicorp/prettier-config"
}
