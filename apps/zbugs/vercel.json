{
  "redirects": [
    {
      "source": "/",
      "has": [
        {
          "type": "host",
          "value": "gigabugs.rocicorp.dev"
        },
        {
          "type": "host",
          "value": "megabugs.rocicorp.dev"
        }
      ],
      "destination": "https://bugs.rocicorp.dev/p/roci",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api"
    },
    {
      "source": "/p/roci/:path*",
      "destination": "/roci.html"
    },
    {
      "source": "/p/:path*",
      "destination": "/index.html"
    },
    {
      "source": "/issue/(.*)",
      "destination": "/index.html"
    }
  ]
}
