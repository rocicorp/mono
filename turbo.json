{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["**/.env.*local"],
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [
        "out/**",
        "dist/**",
        ".next/**",
        "packages/reflect/*.js",
        "packages/reflect/*.d.ts"
      ]
    },
    "lint": {
      "outputs": []
    },
    "format": {},
    "check-format": {
      "outputs": []
    },
    "dev": {
      "cache": true
    },
    "test": {
      "dependsOn": ["build"]
    },
    "test:watch": {
      "cache": false
    },
    "check-types": {
      "dependsOn": ["^check-types", "build"],
      "outputs": []
    }
  }
}
