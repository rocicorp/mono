    "@types/react-dom": "^18.0.6",
    "@vitejs/plugin-react": "^2.0.1",
    "nanoid": "^4.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "replicache-react": "^2.10.0",
    "typescript": "^4.7.4",
    "vite": "^3.0.7"
  },
  "eslintConfig": {
    "extends": "@rocicorp/eslint-config"
  }
