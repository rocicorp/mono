{
  "name": "{{appName}}",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "lint": "eslint --ext .ts,.tsx,.js,.jsx .",
    "dev": "vite",
    "build": "npm run lint && tsc && vite build",
    "preview": "vite preview",
    "format": "prettier --write './src/*/*.{js,jsx,json,ts,tsx,html,css,md}' '*.{cjs,js,jsx,json,ts,tsx,html,css,md}'",
    "watch": "concurrently --kill-others 'npx reflect dev --silence-startup-message' 'VITE_REFLECT_URL=http://127.0.0.1:8080/ npm run dev'"
  },
  "devDependencies": {
    "@rocicorp/eslint-config": "^0.1.2",
    "@rocicorp/rails": "^0.7.0",
    "@rocicorp/reflect": "^{{reflectVersion}}",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@vitejs/plugin-react": "^2.0.1",
    "concurrently": "^8.2.1",
    "nanoid": "^4.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^4.7.4",
    "vite": "^3.0.7"
  },
  "eslintConfig": {
    "extends": "@rocicorp/eslint-config"
  }
}
