{
  "name": "mirror-schema",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "test": "node --experimental-vm-modules ../../node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules ../../node_modules/jest/bin/jest.js --watch",
    "check-types": "tsc --noEmit",
    "format": "prettier --write *",
    "check-format": "prettier --check *",
    "lint": "eslint --ext .ts,.tsx,.js,.jsx src/",
    "deploy-rules-staging": "npx firebase-tools --project staging deploy --only firestore:rules",
    "deploy-rules-prod": "npx firebase-tools --project prod deploy --only firestore:rules"
  },
  "dependencies": {
    "@badrap/valita": "^0.2.0",
    "@google-cloud/firestore": "^6.6.1"
  },
  "devDependencies": {
    "@rocicorp/eslint-config": "^0.4.2",
    "@rocicorp/prettier-config": "^0.1.1",
    "firebase-tools": "^12.3.0",
    "shared": "0.0.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.1.3"
  },
  "exports": {
    ".": "./src/mod.ts",
    "./agent.js": "./src/agent.ts",
    "./app-key.js": "./src/app-key.ts",
    "./app.js": "./src/app.ts",
    "./cloudflare.js": "./src/cloudflare.ts",
    "./deployment.js": "./src/deployment.ts",
    "./membership.js": "./src/membership.ts",
    "./server.js": "./src/server.ts",
    "./team.js": "./src/team.ts",
    "./timestamp.js": "./src/timestamp.ts",
    "./user.js": "./src/user.ts"
  },
  "eslintConfig": {
    "extends": "@rocicorp/eslint-config"
  },
  "prettier": "@rocicorp/prettier-config"
}
