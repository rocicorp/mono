{
  "name": "mirror-schema",
  "private": true,
  "type": "module",
  "module": "./src/mod.ts",
  "types": "./src/mod.ts",
  "scripts": {
    "test": "node --experimental-vm-modules ../../node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules ../../node_modules/jest/bin/jest.js --watch",
    "check-types": "tsc --noEmit",
    "format": "prettier --write *",
    "check-format": "prettier --check *",
    "lint": "eslint --ext .ts,.tsx,.js,.jsx src/",
    "deploy-rules-staging": "npx firebase-tools --project staging deploy --only firestore:rules",
    "deploy-rules-prod": "npx firebase-tools --project prod deploy --only firestore:rules"
  },
  "dependencies": {
    "@badrap/valita": "^0.2.0"
  },
  "devDependencies": {
    "@rocicorp/eslint-config": "^0.4.1",
    "@rocicorp/prettier-config": "^0.1.1",
    "@types/jest": "^29.4.2",
    "shared": "*",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.5"
  },
  "peerDependencies": {
    "firebase-tools": "^12.3.0"
  },
  "exports": {
    ".": {
      "import": "./src/mod.ts",
      "types": "./src/mod.ts"
    },
    "./agent.js": {
      "import": "./src/agent.ts",
      "types": "./src/agent.ts"
    },
    "./app-key.js": {
      "import": "./src/app-key.ts",
      "types": "./src/app-key.ts"
    },
    "./app.js": {
      "import": "./src/app.ts",
      "types": "./src/app.ts"
    },
    "./cloudflare.js": {
      "import": "./src/cloudflare.ts",
      "types": "./src/cloudflare.ts"
    },
    "./deployment.js": {
      "import": "./src/deployment.ts",
      "types": "./src/deployment.ts"
    },
    "./membership.js": {
      "import": "./src/membership.ts",
      "types": "./src/membership.ts"
    },
    "./server.js": {
      "import": "./src/server.ts",
      "types": "./src/server.ts"
    },
    "./team.js": {
      "import": "./src/team.ts",
      "types": "./src/team.ts"
    },
    "./timestamp.js": {
      "import": "./src/timestamp.ts",
      "types": "./src/timestamp.ts"
    },
    "./user.js": {
      "import": "./src/user.ts",
      "types": "./src/user.ts"
    }
  },
  "eslintConfig": {
    "extends": "@rocicorp/eslint-config"
  },
  "prettier": "@rocicorp/prettier-config"
}
