{
  "name": "reflect-client",
  "description": "",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "format": "prettier --write *",
    "check-format": "prettier --check *",
    "lint": "eslint --ext .ts,.tsx,.js,.jsx src/",
    "test": "web-test-runner",
    "test:watch": "web-test-runner --watch",
    "check-types": "tsc --noEmit",
    "build-dts": "rm -rf out/.dts/ && tsc --emitDeclarationOnly --outDir out/.dts/ && rollup --config rollup.config.js && rm -rf out/.dts",
    "build-min": "node tool/build.js",
    "build": "rm -rf out && npm run build-dts && npm run build-min && node tool/check-ambient-context.js"
  },
  "dependencies": {
    "@badrap/valita": "^0.2.0",
    "@rocicorp/lock": "^1.0.1",
    "@rocicorp/logger": "^5.2.0",
    "@rocicorp/resolver": "^1.0.0",
    "replicache": "13.0.0-beta.1"
  },
  "devDependencies": {
    "@rocicorp/eslint-config": "^0.4.2",
    "@rocicorp/prettier-config": "^0.1.1",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@types/mocha": "^10.0.1",
    "@types/sinon": "^10.0.11",
    "@web/dev-server": "^0.1.36",
    "@web/dev-server-esbuild": "^0.3.4",
    "@web/test-runner": "^0.15.1",
    "@web/test-runner-playwright": "^0.9.0",
    "chai": "^5.0.0-alpha.0",
    "datadog": "0.0.0",
    "esbuild": "^0.18.3",
    "reflect-protocol": "0.0.0",
    "reflect-shared": "0.0.0",
    "reflect-types": "0.0.0",
    "rollup": "^3.24.0",
    "rollup-plugin-dts": "^5.3.0",
    "shared": "0.0.0",
    "sinon": "^13.0.1",
    "typescript": "^5.1.3"
  },
  "eslintConfig": {
    "extends": "@rocicorp/eslint-config"
  },
  "prettier": "@rocicorp/prettier-config"
}
