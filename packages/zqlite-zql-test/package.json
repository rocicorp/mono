    "typescript": "~5.7.3",
    "vitest": "3.0.8"
  },
  "eslintConfig": {
    "extends": "../../eslint-config.json"
  },
  "prettier": "@rocicorp/prettier-config",
  "dependencies": {
    "zql": "0.0.0",
    "zqlite": "0.0.0"
  }
}
