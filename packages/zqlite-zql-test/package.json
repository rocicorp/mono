{
  "name": "zqlite-zql-test",
  "description": "zql tests against zqlite TableSource",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "format": "prettier --write .",
    "check-format": "prettier --check .",
    "check-types": "tsc",
    "check-types:watch": "tsc --watch",
    "lint": "eslint --ext .ts,.tsx,.js,.jsx src/",
