{
  "name": "reflect-shared",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "format": "prettier --write *",
    "check-format": "prettier --check *",
    "lint": "eslint --ext .ts,.tsx,.js,.jsx src/",
    "build": "rm -rf out && npm run build-dts",
    "build-dts": "rm -rf out/.dts/ && tsc --emitDeclarationOnly --outDir out/.dts/ && rollup --config rollup.config.js && rm -rf out/.dts",
    "check-types": "tsc --noEmit",
    "test": "node --no-warnings --experimental-vm-modules ../../node_modules/jest/bin/jest.js",
    "test:watch": "node --no-warnings --experimental-vm-modules ../../node_modules/jest/bin/jest.js --watch"
  },
  "devDependencies": {
    "@rocicorp/eslint-config": "^0.5.0",
    "@rocicorp/prettier-config": "^0.1.1",
    "esbuild": "^0.18.3",
    "replicache": "13.0.1",
    "rollup": "^3.24.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.2.2"
  },
  "exports": {
    ".": "./src/mod.ts",
    "./tool/get-version.js": "./tool/get-version.js"
  },
  "eslintConfig": {
    "extends": "@rocicorp/eslint-config"
  },
  "prettier": "@rocicorp/prettier-config"
}
