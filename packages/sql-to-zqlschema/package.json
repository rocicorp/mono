{
  "name": "sql-to-zqlschema",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "test:pg17": "vitest --project='*17*' run",
    "test:pg17:watch": "vitest --project='*17*'",
    "test:no-pg": "vitest --project='*no-pg*' run",
    "test:no-pg:watch": "vitest --project='*no-pg*'",
    "check-types": "tsc",
    "check-types:watch": "tsc --watch",
    "format": "prettier --write .",
    "check-format": "prettier --check .",
    "lint": "oxlint --quiet --type-aware src/"
  },
  "dependencies": {
    "postgres": "^3.4.4"
  },
  "devDependencies": {
    "@rocicorp/prettier-config": "^0.4.0",
    "@testcontainers/postgresql": "^10.13.0",
    "@types/node": "^22.10.5",
    "shared": "0.0.0",
    "typescript": "~5.9.3",
    "vitest": "4.0.15"
  },
  "prettier": "@rocicorp/prettier-config"
}
